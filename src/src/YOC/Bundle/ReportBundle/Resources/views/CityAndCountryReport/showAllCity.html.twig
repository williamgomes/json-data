{% extends 'base.html.twig' %}

{% block name %}City List{% endblock %}

{% block body %}
    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Country ID</th>
            <th scope="col">City Name</th>
        </tr>
        </thead>
        <tbody>
        {% for oCity in oCities %}
        <tr>
            <th scope="row">{{ oCity.Id }}</th>
            <td>{{ oCity.countryId }}</td>
            <td>{{ oCity.cityName }}</td>
        </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}

